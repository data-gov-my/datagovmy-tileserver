{
    "options": {
        "paths": {
            "fonts": "/data/fonts",
            "styles": "/styles",
            "sprites": "/styles",
            "mbtiles": "/data"
        },
        "domains": ["localhost:8080", "127.0.0.1:8080"],
        "formatQuality": {
            "jpeg": 80,
            "webp": 90
        }
    },
    "styles": {
        "light": {
            "style": "light/style.json",
            "tilejson": {
                "type": "overlay"
            }
        },
        "dark": {
            "style": "dark/style.json",
            "tilejson": {
                "type": "overlay"
            }
        }
    },
    "data": {
        "v3": {
            "mbtiles": "malaysia-singapore-brunei.mbtiles"
        },
        "contour": {
            "mbtiles": "contour.mbtiles"
        }
    }
}
